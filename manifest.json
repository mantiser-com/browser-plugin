{
  "manifest_version": 2,

  "name": "Mantiser",
  "description": "This plugin will send the current url to Mantiser",
  "version": "1.0",

  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": ["storage", "activeTab","https://mantiser.com/*"],
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  }
}
